# THOSE VALUES ARE EDITABLE
CEB_SUPABASE_ID="your-supabase-project-id"
CEB_SUPABASE_ANON_KEY="your-supabase-anon-key"
CEB_SUPABASE_URL="https://your-project.supabase.co"
CEB_SUPABASE_SOCKET_URL="wss://your-project.supabase.co/realtime/v1/websocket?apikey=your-anon-key&vsn=1.0.0"


CEB_CLERK_PUBLISHABLE_KEY=pk_test_your-clerk-key


# Use Shared connection pooler (supports both IPv4/IPv6)
DATABASE_URL="postgresql://postgres.your-project:your-password@aws-0-us-west-1.pooler.supabase.com:6543/postgres"
SUPABASE_DB_PASSWORD=your-password

# Frontend Supabase Connection
VITE_SUPABASE_URL="https://your-project.supabase.co"
VITE_SUPABASE_ANON_KEY="your-supabase-anon-key"

# If your network supports IPv6 or you purchased IPv4 addon, use dedicated pooler
# DATABASE_URL="postgres://postgres:[YOUR-PASSWORD]@db.your-project.supabase.co:6543/postgres"

# OpenRouter API Key (replaces OpenAI)
# Get your key from https://openrouter.ai/keys
OPENROUTER_API_KEY="your-openrouter-api-key"

# Site URL for OpenRouter HTTP-Referer header
SITE_URL="https://captur.academy"

# Supadata.ai API for YouTube transcripts
SUPADATA_API_KEY="your-supadata-api-key"

# Chrome Extension ID
# Get from chrome://extensions/ after loading unpacked extension
# Different .env files use different IDs:
# - .env -> development extension ID
# - .env.production -> staging/testing extension ID
# - .env.release -> Chrome Web Store extension ID
VITE_CHROME_EXTENSION_ID="your-extension-id"
